  "html": "{% set tel_vayda_lst = frappe.get_list(\n    \"Purchase Order\",\n    {\"supplier\":doc.supplier, \"order_type\":\"TEL VAYDA\", \"per_received\":[\"<\", \"100\"]},\n    [\"transaction_date\",\n    \"total_qty\", \"qty_billed\"])\n%}\n\n{% set total_qty   = tel_vayda_lst | sum(attribute='total_qty')%}\n{% set total_bill  = tel_vayda_lst | sum(attribute='qty_billed')%}\n\n{% set total_tel = total_qty - total_bill %}\n\n{% set phone_vayda_lst = frappe.get_list(\n    \"Purchase Order\",\n    {\"supplier\":doc.supplier, \"order_type\":\"PHONE VAYDA\", \"per_received\":[\"<\", \"100\"]},\n    [\"transaction_date\",\n    \"total_qty\", \"qty_billed\", \"grand_total\", \"per_received\", \"per_billed\"])\n%}\n\n\n{% set total_phone  = [] %}\n\n\n<div class=\"row section-break\">\n    <div class=\"col-xs-12 column-break\">\n        <h3 class=\"text-center\">{{_(\"PURCHASE SUMMARY\")}}</h3>\n    </div>\n</div>\n<br>\n<div class=\"row section-break\">\n    <div class=\"col-xs-12 column-break\">\n        <hr>\n    </div>\n</div>\n<div class=\"row section-break\">\n    <div class=\"col-xs-6 column-break\">\n        <div class=\"row important data-field\" data-fieldname=\"supplier_name\" data-fieldtype=\"Data\">\n            <div class=\"col-xs-5\">\n\n                <label>{{_(\"Supplier Name\")}}:</label>\n            </div>\n            <div class=\"col-xs-7 value\">\n                {{doc.supplier_name}}\n            </div>\n        </div>\n    </div>\n\n    <div class=\"col-xs-6 column-break\">\n        <div class=\"row  data-field\" data-fieldname=\"transaction_date\" data-fieldtype=\"Date\">\n            <div class=\"col-xs-5\">\n                <label>{{_(\"Pending\")}}:</label>\n            </div>\n            <div class=\"col-xs-7 value\">\n                <b> {{\"\u20b9 {:,.2f}\".format(total_phone| float + total_tel| float)}} </b>\n            </div>\n        </div>\n    </div>\n</div>\n<div class=\"row section-break\">\n    <div class=\"col-xs-12 column-break\">\n        <hr>\n    </div>\n</div>\n<div class=\"row section-break\">\n    <div class=\"col-xs-8 column-break\">\n        <div data-fieldname=\"phone_vayda\" data-fieldtype=\"Table\">\n            <table class=\"table table-bordered table-condensed\">\n                <thead>\n                    <tr>\n                        <th colspan=\"4\" class=\"text-center\" data-fieldname=\"items\" data-fieldtype=\"Table\">\n                            PHONE VAYDA</th>                        \n                    </tr>\n                    <tr>\n                        <th style=\"width: 150px;\" class=\"text-center\" data-fieldname=\"items\" data-fieldtype=\"Table\">\n                            {{_(\"Date\")}}</th>\n                        <th style=\"width: 80px;\" class=\"text-center\" data-fieldname=\"items\" data-fieldtype=\"Table\">\n                            {{_(\"Quantity\")}}</th>\n                        <th style=\"width: 150px;\" class=\"text-center\" data-fieldname=\"items\" data-fieldtype=\"Table\">\n                            {{_(\"Rate\")}}</th>\n                        <th style=\"width: 150px;\" class=\"text-center\" data-fieldname=\"items\" data-fieldtype=\"Table\">\n                            {{_(\"Amount\")}}</th>\n                    </tr>\n                </thead>\n                <tbody>\n                    {% for row in phone_vayda_lst%}\n                    <tr>\n                        <td class=\"\" data-fieldname=\"items\" data-fieldtype=\"Date\">\n                            <div class=\"value text-center\">\n                                {{row.transaction_date}}\n                            </div>\n                        </td>\n\n                        <td class=\"\" data-fieldname=\"items\" data-fieldtype=\"Table\">\n                            <div class=\"value text-center\">\n                                {{row.total_qty - row.qty_billed}}\n                            </div>\n                        </td>\n\n                        <td class=\"text-right\" data-fieldname=\"items\" data-fieldtype=\"Table\">\n                            <div class=\"value\">\n                                -\n                            </div>\n                        </td>\n                        <td class=\"text-right\" data-fieldname=\"items\" data-fieldtype=\"Table\">\n                            <div class=\"value\">\n                                {% set total = row.grand_total | float  * ((100.0 - row.per_billed | float) / 100.0)%}\n                                \n                                {% if  not total_phone.append({\"total\": total}) %}\n                                    {{\"\u20b9 {:,.2f}\".format(total)}}\n                                {% endif %}\n                            </div>\n                        </td>\n                    </tr>\n                    {% endfor %}\n                    <tr>\n                        <td colspan=\"3\" class=\"text-center\" data-fieldname=\"items\" data-fieldtype=\"Table\">\n                            <div class=\"value\"> <b>TOTAL</b> </div>\n                        </td>\n\n                        <td class=\"text-right\" data-fieldname=\"items\" data-fieldtype=\"Table\">\n                            <div class=\"value\">\n\n                              <b>{{\"\u20b9 {:,.2f}\".format(total_phone | sum(attribute='total'))}}</b>\n                            </div>\n                        </td>\n                    </tr>\n                </tbody>\n            </table>\n        </div>\n\n    </div>\n        <div class=\"col-xs-4 column-break\">\n        <div data-fieldname=\"Tel_vayda\" data-fieldtype=\"Table\">\n            <table class=\"table table-bordered table-condensed\">\n                <thead>\n                    <tr>\n                        <th colspan=\"4\" class=\"text-center\" data-fieldname=\"items\" data-fieldtype=\"Table\">\n                            TEL VAYDA</th>                      \n                    </tr>\n                    <tr>\n                        <th style=\"width: 150px;\" class=\"text-center\" data-fieldname=\"items\" data-fieldtype=\"Table\">\n                            {{_(\"Date\")}}</th>\n\n                        <th style=\"width: 150px;\" class=\"text-center\" data-fieldname=\"items\" data-fieldtype=\"Table\">\n                            {{_(\"Quantity\")}}</th>\n                    </tr>\n                </thead>\n                <tbody>\n                    {% for row in tel_vayda_lst%}\n                    <tr>\n                        <td class=\"\" data-fieldname=\"items\" data-fieldtype=\"Table\">\n                            <div class=\"value text-center\">\n                                {{row.transaction_date}}\n                            </div>\n                        </td>\n\n                        <td class=\"\" data-fieldname=\"items\" data-fieldtype=\"Table\">\n                            <div class=\"value text-center\">\n                                {{row.total_qty - row.qty_billed}}\n                            </div>\n                        </td>\n                    </tr>\n                    {% endfor %}\n                    <tr>\n                        <td class=\"\" data-fieldname=\"items\" data-fieldtype=\"Table\">\n                            <div class=\"value text-center\"> <b>TOTAL</b> </div>\n                        </td>\n                        <td class=\"\" data-fieldname=\"items\" data-fieldtype=\"Table\">\n                            <div class=\"value text-center\">\n                                <b>{{total_tel}}</b>\n                            </div>\n                        </td>\n                    </tr>\n\n                </tbody>\n            </table>\n        </div>\n\n    </div>\n\n</div>", 
